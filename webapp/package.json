    "webpack-merge": "^5.9.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0",
    "workbox-webpack-plugin": "^7.0.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

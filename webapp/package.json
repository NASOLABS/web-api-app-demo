{
  "name": "web-api-app-demo",
  "version": "0.1.0",
  "private": true,
  "description": "Web Api App Demo using React",
  "scripts": {
    "oostart": "react-scripts start",
    "oobuild": "react-scripts build",
    "ootest": "react-scripts test",
    "ooeject": "react-scripts eject",
    "test": "npm test",
    "setup": "source setup.sh",
    "analyze": "webpack -p --mode=production --config webpack/webpack.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "start": "source .env && npm run webpack:dev",
    "build": "API_KEY=$API_KEY SENDER_ID=$SENDER_ID APP_ID=$APP_ID NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --port=9060",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 127.0.0.1 "
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "react": "^18.2.0",
    "react-axios": "^2.0.6",
    "react-dom": "^18.2.0",
    "react-scripts": "^4.0.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.4",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "image-webpack-loader": "^5.0.0",
    "jest": "^25.0.0",
    "lint-staged": "^9.2.5",
    "lodash-webpack-plugin": "^0.11.5",
    "merge-jsons-webpack-plugin": "^1.0.19",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.13.0",
